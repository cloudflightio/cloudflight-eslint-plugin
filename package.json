{
    "name": "@cloudflight/eslint-plugin",
    "version": "1.0.0-rc5",
    "private": true,
    "volta": {
        "node": "20.12.2",
        "yarn": "4.2.1"
    },
    "workspaces": [
        "packages/*"
    ],
    "homepage": "https://github.com/cloudflightio/cloudflight-eslint-plugin",
    "bugs": {
        "url": "https://github.com/cloudflightio/cloudflight-eslint-plugin/issues"
    },
    "description": "",
    "scripts": {
        "build": "yarn workspaces foreach -pA --topological-dev run build-package",
        "lint": "eslint .",
        "test": "yarn workspaces foreach -pA run test-package",
        "postinstall": "husky",
        "publish-workspaces": "node ./scripts/prepare.js && yarn workspaces foreach -A --no-private npm publish"
    },
    "keywords": [
        "eslint",
        "eslintplugin"
    ],
    "author": "Cloudflight GmbH",
    "license": "Apache-2.0",
    "devDependencies": {
        "@eslint/compat": "1.2.0",
        "@eslint/js": "9.11.1",
        "@types/eslint": "9.6.1",
        "@types/eslint__js": "8.42.3",
        "@types/estree": "1.0.6",
        "@types/jest": "29.5.13",
        "@types/node": "20.16.10",
        "@typescript-eslint/eslint-plugin": "8.8.0",
        "@typescript-eslint/parser": "8.8.0",
        "@typescript-eslint/utils": "8.8.0",
        "@vue/eslint-config-typescript": "14.0.0",
        "angular-eslint": "18.3.1",
        "eslint": "9.11.1",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-json-es": "1.6.0",
        "eslint-plugin-jsx-a11y": "6.8.0",
        "eslint-plugin-n": "17.10.3",
        "eslint-plugin-no-unsanitized": "4.1.2",
        "eslint-plugin-react": "7.34.1",
        "eslint-plugin-react-hooks": "4.6.2",
        "eslint-plugin-rxjs": "5.0.3",
        "eslint-plugin-security": "3.0.1",
        "eslint-plugin-vue": "9.28.0",
        "husky": "9.1.6",
        "jest": "29.7.0",
        "lint-staged": "15.2.10",
        "ts-jest": "29.2.5",
        "ts-node": "10.9.2",
        "typescript": "5.5.4",
        "typescript-eslint": "8.8.0"
    },
    "lint-staged": {
        "*.js": "echo 'Not Implemented'",
        "*.ts": "echo 'Not Implemented'"
    }
}
