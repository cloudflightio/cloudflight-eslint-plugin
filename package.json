{
    "name": "@cloudflight/eslint-plugin",
    "version": "0.15.1",
    "private": true,
    "volta": {
        "node": "18.14.0",
        "yarn": "4.0.0-rc.43"
    },
    "workspaces": [
        "packages/*"
    ],
    "homepage": "https://github.com/cloudflightio/cloudflight-eslint-plugin",
    "bugs": {
        "url": "https://github.com/cloudflightio/cloudflight-eslint-plugin/issues"
    },
    "description": "",
    "scripts": {
        "build": "yarn workspaces foreach -t run build-package",
        "lint": "eslint . && prettier --check .",
        "test": "yarn workspaces foreach run test-package",
        "postinstall": "husky install",
        "publish-workspaces": "node ./scripts/prepare.js && yarn workspaces foreach --no-private npm publish"
    },
    "keywords": [
        "eslint",
        "eslintplugin"
    ],
    "author": "Cloudflight GmbH",
    "license": "Apache-2.0",
    "devDependencies": {
        "@angular-eslint/eslint-plugin": "15.2.1",
        "@angular-eslint/eslint-plugin-template": "15.2.1",
        "@angular-eslint/template-parser": "15.2.1",
        "@rushstack/eslint-patch": "1.2.0",
        "@types/eslint": "8.37.0",
        "@types/estree": "1.0.1",
        "@types/jest": "29.5.1",
        "@types/node": "18.16.3",
        "@typescript-eslint/eslint-plugin": "5.59.2",
        "@typescript-eslint/parser": "5.59.2",
        "@vue/eslint-config-prettier": "7.1.0",
        "@vue/eslint-config-typescript": "11.0.3",
        "eslint": "8.39.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-json-es": "1.5.7",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-no-unsanitized": "4.0.2",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-rxjs": "5.0.3",
        "eslint-plugin-security": "1.7.1",
        "eslint-plugin-vue": "9.11.0",
        "husky": "8.0.3",
        "jest": "29.5.0",
        "prettier": "2.8.8",
        "pretty-quick": "3.1.3",
        "ts-jest": "29.1.0",
        "ts-node": "10.9.1",
        "typescript": "5.0.4"
    }
}
