{
    "name": "@cloudflight/eslint-plugin",
    "version": "0.9.0",
    "private": true,
    "volta": {
        "node": "18.14.0",
        "yarn": "3.4.1"
    },
    "workspaces": [
        "packages/*"
    ],
    "homepage": "https://github.com/cloudflightio/cloudflight-eslint-plugin",
    "bugs": {
        "url": "https://github.com/cloudflightio/cloudflight-eslint-plugin/issues"
    },
    "description": "",
    "scripts": {
        "build": "yarn workspaces foreach -t run build-package",
        "lint": "eslint .",
        "test": "yarn workspaces foreach run test-package",
        "postinstall": "husky install",
        "publish-workspaces": "node ./scripts/prepare.js && yarn workspaces foreach --no-private npm publish"
    },
    "keywords": [
        "eslint",
        "eslintplugin"
    ],
    "author": "Cloudflight GmbH",
    "license": "Apache-2.0",
    "devDependencies": {
        "@angular-eslint/eslint-plugin": "15.2.1",
        "@angular-eslint/eslint-plugin-template": "15.2.1",
        "@angular-eslint/template-parser": "15.2.1",
        "@rushstack/eslint-patch": "1.2.0",
        "@types/eslint": "8.21.0",
        "@types/estree": "1.0.0",
        "@types/jest": "29.4.0",
        "@types/node": "18.13.0",
        "@typescript-eslint/eslint-plugin": "5.51.0",
        "@typescript-eslint/parser": "5.51.0",
        "@vue/eslint-config-prettier": "7.0.0",
        "@vue/eslint-config-typescript": "11.0.2",
        "eslint": "8.34.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-json-es": "1.5.7",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-no-unsanitized": "4.0.2",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-react": "7.31.11",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-security": "1.5.0",
        "eslint-plugin-vue": "9.9.0",
        "husky": "8.0.3",
        "jest": "29.4.3",
        "prettier": "2.8.4",
        "pretty-quick": "3.1.3",
        "ts-jest": "29.0.5",
        "ts-node": "10.9.1",
        "typescript": "4.9.5"
    }
}
