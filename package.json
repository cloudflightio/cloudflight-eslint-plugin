{
    "name": "@cloudflight/eslint-plugin",
    "version": "0.5.0",
    "private": true,
    "volta": {
        "node": "18.12.1",
        "yarn": "3.2.4"
    },
    "workspaces": [
        "packages/*"
    ],
    "homepage": "https://github.com/cloudflightio/cloudflight-eslint-plugin",
    "bugs": {
        "url": "https://github.com/cloudflightio/cloudflight-eslint-plugin/issues"
    },
    "description": "",
    "scripts": {
        "build": "yarn workspaces foreach -t run build-package",
        "lint": "eslint .",
        "test": "yarn workspaces foreach run test-package",
        "postinstall": "husky install && yarn run build",
        "publish-workspaces": "node ./scripts/prepare.js && yarn workspaces foreach npm publish"
    },
    "keywords": [
        "eslint",
        "eslintplugin"
    ],
    "author": "Cloudflight GmbH",
    "license": "Apache-2.0",
    "devDependencies": {
        "@angular-eslint/eslint-plugin": "14.1.2",
        "@angular-eslint/eslint-plugin-template": "14.1.2",
        "@angular-eslint/template-parser": "14.1.2",
        "@rushstack/eslint-patch": "1.2.0",
        "@types/eslint": "8.4.10",
        "@types/estree": "1.0.0",
        "@types/jest": "29.2.2",
        "@types/node": "16.18.3",
        "@typescript-eslint/eslint-plugin": "5.42.1",
        "@typescript-eslint/parser": "5.42.1",
        "@vue/eslint-config-prettier": "7.0.0",
        "@vue/eslint-config-typescript": "11.0.2",
        "eslint": "8.27.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-json-es": "1.5.7",
        "eslint-plugin-vue": "9.7.0",
        "husky": "8.0.1",
        "jest": "29.3.1",
        "prettier": "2.7.1",
        "pretty-quick": "3.1.3",
        "ts-jest": "29.0.3",
        "ts-node": "10.9.1",
        "typescript": "4.8.4"
    }
}
