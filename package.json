{
    "name": "@cloudflight/eslint-plugin",
    "version": "0.20.1",
    "private": true,
    "volta": {
        "node": "18.17.1",
        "yarn": "4.0.0-rc.44"
    },
    "workspaces": [
        "packages/*"
    ],
    "homepage": "https://github.com/cloudflightio/cloudflight-eslint-plugin",
    "bugs": {
        "url": "https://github.com/cloudflightio/cloudflight-eslint-plugin/issues"
    },
    "description": "",
    "scripts": {
        "build": "yarn workspaces foreach -pt run build-package",
        "lint": "eslint .",
        "test": "yarn workspaces foreach -p run test-package",
        "postinstall": "husky install && git update-index --chmod=+x ./.husky/pre-commit",
        "publish-workspaces": "node ./scripts/prepare.js && yarn workspaces foreach --no-private npm publish"
    },
    "keywords": [
        "eslint",
        "eslintplugin"
    ],
    "author": "Cloudflight GmbH",
    "license": "Apache-2.0",
    "devDependencies": {
        "@angular-eslint/bundled-angular-compiler": "16.1.1",
        "@angular-eslint/eslint-plugin": "16.1.1",
        "@angular-eslint/eslint-plugin-template": "16.1.1",
        "@angular-eslint/template-parser": "16.1.1",
        "@angular/compiler": "16.2.3",
        "@rushstack/eslint-patch": "1.3.3",
        "@types/eslint": "8.44.2",
        "@types/estree": "1.0.1",
        "@types/jest": "29.5.4",
        "@types/node": "18.17.12",
        "@typescript-eslint/eslint-plugin": "6.5.0",
        "@typescript-eslint/parser": "6.5.0",
        "@vue/eslint-config-typescript": "11.0.3",
        "eslint": "8.48.0",
        "eslint-plugin-import": "2.28.1",
        "eslint-plugin-json-es": "1.5.7",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-no-unsanitized": "4.0.2",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-rxjs": "5.0.3",
        "eslint-plugin-security": "1.7.1",
        "eslint-plugin-vue": "9.17.0",
        "husky": "8.0.3",
        "jest": "29.6.4",
        "lint-staged": "14.0.1",
        "ts-jest": "29.1.1",
        "ts-node": "10.9.1",
        "typescript": "5.2.2"
    },
    "lint-staged": {
        "*.js": "eslint --config .eslintrc.format.js --fix",
        "*.ts": "eslint --config .eslintrc.format.js --fix"
    }
}
